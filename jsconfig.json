{
  "compilerOptions": {
    "baseUrl": "./src/",
    "paths": {
      "*": ["*"],
      "images/*": ["src/images/*"],
      "components/*": ["src/components/*"],
      "services/*": ["src/services/*"],
      "scenes/*": ["src/scenes/*"],
      "configs/*": ["src/configs/*"]
    },
    "target": "es2017",
    "allowSyntheticDefaultImports": false
  },
  "exclude": ["node_modules", "build"]
}
